if not _G.allreadyantiillegalroomcopevent then
_G.allreadyantiillegalroomcopevent = true

local mt = getrawmetatable(game)
make_writeable(mt)

local namecall = mt.__namecall
mt.__namecall = newcclosure(function(self, ...)
    local method = getnamecallmethod()
    local args = {...}

    if method == "FireServer" and tostring(self) == "CopEvent" then
        if _G.antiillegalroom then
        if args[1] == "IllegalRoom" then
           return
        end
        end
    end
    return namecall(self, table.unpack(args))
end)

else
end

Toggles:AddToggle({
    Name = "Inf Energy",
    Default = false,
    Callback = function(infstam)
        spawn(function()
            if infstam then
                _G.infenegy = true
                local assscript = game.Players.LocalPlayer.Backpack.Local:FindFirstChild("CombatMain")
                    for i,garbage in next, getgc() do
                        if type(garbage) == "function" and getfenv(garbage).script == assscript then
                        local Name = getinfo(garbage).name
                        if Name == "AddStamina" then
                            _G.stamfunction = garbage
                            setupvalue(garbage,1,math.huge)
                        end
                    end
                end
                setupvalue(_G.stamfunction, 1, math.huge)
                notify("Life Sentence Fucker","Inf Energy On")
            else   
                _G.infenegy = false
                setupvalue(_G.stamfunction, 1, 100)
                notify("Life Sentence Fucker","Inf Energy Off")
            end 
        while _G.infenegy do task.wait()
            pcall(function()
            if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Health <= 0 then
                task.wait(6)
                local assscript = game.Players.LocalPlayer.Backpack.Local:FindFirstChild("CombatMain")
                    for i,garbage in next, getgc() do
                        if type(garbage) == "function" and getfenv(garbage).script == assscript then
                        local Name = getinfo(garbage).name
                        if Name == "AddStamina" then
                            _G.stamfunction = garbage
                            setupvalue(garbage,1,math.huge)
                        end
                    end
                end
                end
            end)
            end
        end)
    end
})

