Toggles:AddToggle({
    Name = "Auto Reload FireArm",
    Default = false,
    Callback = function(areload)
            if areload then
                if not _G.autoreloadfirsttime then
                    _G.autoreloadfirsttime = true
                    local newmsg = Instance.new("Message",workspace)
                    newmsg.Text = "This will only reload the firearm that u have equipped when it reaches 0 ammo!"
                    task.wait(6)
                    newmsg:Destroy()
                end
               _G.autoreload = true
               notify("Life Sentence Fucker","Auto Reload On")
            else
                _G.autoreload = false
                notify("Life Sentence Fucker","Auto Reload Off")
            end   
        while _G.autoreload do task.wait(0.1)
            pcall(function()
                if game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
                    if game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Clip") then
                        clip = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Clip").Value
                    end
                end
                
                if clip == 0 then
                    if game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
                        if game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Reserve") then
                            reserve = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Reserve").Value
                        end
                    end
                    if reserve > 0 then
                        game:GetService("ReplicatedStorage").Events.WeaponEvent:FireServer("Reload")
                    end    
                end
            end)
        end
    end
})


