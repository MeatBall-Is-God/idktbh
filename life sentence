if not _G.allreadysilentaimnamecall then
_G.allreadysilentaimnamecall = true

local OldNameCall = nil
OldNameCall = hookmetamethod(game, "__namecall", function(Self, ...)
    local Args = {...}
    local NamecallMethod = getnamecallmethod()
    if NamecallMethod == "FireServer" and Args[1] == "Fire" and _G.silentaim and typeof(Args[2]) == "table" and typeof(Args[2][2]) == "Instance" then
        local Target = SilentAimTarget()
        if Target ~= false then
            for _, A_1 in next, Args[2] do
                if typeof(A_1) == "Instance" then
                    Args[2][_] = Target
                end
            end
        end
    end
    return OldNameCall(Self, unpack(Args))
end)

else
end

