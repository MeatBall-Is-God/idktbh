game:GetService("RunService").Heartbeat:Connect(function()
    pcall(function()
        --- anti noclip bypass
        if not game.Players.LocalPlayer.Backpack.Local.CombatMain:FindFirstChild("Bypassedantinoclip") then
        local n = Instance.new("Model",game.Players.LocalPlayer.Backpack.Local.CombatMain)
        n.Name = "Bypassedantinoclip"
        local anticlip = getsenv(game.Players.LocalPlayer.Backpack.Local.CombatMain).CheckClip
        hookfunction(anticlip, function()
        warn("Not flagged cuz i bypassed anti noclip")
        end)
        end
        --- im not gonna slow down my character bc i shot my gun
        if not game.Players.LocalPlayer.Backpack.Local.CombatMain:FindFirstChild("Noslowdowncharacterbruh") then
        local n2 = Instance.new("Model",game.Players.LocalPlayer.Backpack.Local.CombatMain)
        n2.Name = "Noslowdowncharacterbruh"
        local gunslow = getsenv(game.Players.LocalPlayer.Backpack.Local.CombatMain).SlowPlayer
        hookfunction(gunslow, function()
        warn("Sorry but im not gonna slow down my character bc i shot my gun")
        end)
        end
        --- anti tp bypass
        if game.Players.LocalPlayer.Backpack.Stats.TeleportPass.Value == false then
            game.Players.LocalPlayer.Backpack.Stats.TeleportPass.Value = true
        end
        --- literally atleast 75% of the entire anti cheat is in that one script (btw it literally has nothing to do with the death screen)
        if game.Players.LocalPlayer.Backpack.Local:FindFirstChild("Dead") then
            game.Players.LocalPlayer.Backpack.Local.Dead:Destroy()
        end    
    end)
end)

end)
